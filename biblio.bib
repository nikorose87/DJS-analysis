@article{Fukuchi2014,
abstract = {Background The increased injury risk in older runners has been associated with alterations in muscle strength, flexibility, and gait biomechanics. This study investigated whether older runners exhibit changes in muscle strength, flexibility and running biomechanics compared to younger runners, and possible relationships between these changes. Methods Thirty-five young (20-36 yrs) and 35 older (55-71 yrs) recreational runners participated in the study. Measures of three-dimensional biomechanical data during treadmill running at 2.7 m/s and measures of muscle strength and flexibility were compared between groups. A correlation analysis between biomechanical and clinical variables was also performed. Findings Older runners demonstrated an overall reduction in muscle strength and flexibility, and altered running patterns compared to young runners but correlations between clinical and biomechanical variables were scarce. Reduced hip, ankle and trunk excursions along with reduced knee and ankle positive work were found in older runners. Older runners also exhibited increased knee abduction impulse, ankle abduction impulse and vertical loading rates. In contrast, older runners did not present a distal-to-proximal lower extremity joint moment redistribution. Interpretation We observed age-related reduced strength and flexibility concomitant with alterations in running biomechanics, but a lack of correlation between these variables. This finding hampers the use of single, or even a subset of characteristics to better understand age-related changes in runners. The observed changes are complex and multivariate in nature. Clinicians will most likely have to monitor both clinical and biomechanical characteristics to optimize care. However, future studies need to prospectively address what are biomechanical age-related risk factors in runners. {\textcopyright} 2013 Elsevier Ltd.},
author = {Fukuchi, Reginaldo K. and Stefanyshyn, Darren J. and Stirling, Lisa and Duarte, Marcos and Ferber, Reed},
doi = {10.1016/j.clinbiomech.2013.12.007},
file = {::},
issn = {1879-1271},
journal = {Clinical Biomechanics},
keywords = {Aging,Biomechanics,Running},
month = {mar},
number = {3},
pages = {304--310},
pmid = {24380685},
publisher = {Elsevier Ltd},
title = {{Flexibility, muscle strength and running biomechanical adaptations in older runners}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24380685},
volume = {29},
year = {2014}
}
@article{Duvinage2012,
abstract = {Current active leg prostheses do not integrate the most$\backslash$nrecent advances in Brain-Computer Interfaces (BCI) and$\backslash$nbipedal robotics. Moreover, their actuators are seldom$\backslash$ndriven by the subjects intention.$\backslash$nIn this paper, assuming high-level commands reflecting$\backslash$nusers intention are available (such as accelerate, decelerate$\backslash$nor stop), we propose an original and biologically-inspired$\backslash$nleg prosthesis control system. A Programmable Central$\backslash$nPattern Generator (PCPG), which is able to model human$\backslash$nwalk in a perfectly periodic way, generates the output$\backslash$nsignal to control the prosthesis. One of the main interests$\backslash$nof that tool is the possibility to modify the walking pattern$\backslash$nlearned at a middle speed and to adapt it to different$\backslash$nwalking speeds in a smooth way.$\backslash$nWhat is proposed in this paper is to exploit this feature$\backslash$nand increase the comfort of the patient thanks to$\backslash$na specific tuning of the PCPG parameters relying on$\backslash$nreal kinematics of a similar subject. The results of a$\backslash$nstudy for one subject are presented and we show how$\backslash$nto modify at best the PCPG parameters. A fourth-order$\backslash$npolynomial interpolation between the PCPG parameters$\backslash$nand the speeds provides good similarity indices between$\backslash$nreal walk and generated patterns. The values of these$\backslash$nindices are presented for different walking speeds.$\backslash$nKeywords: BNCI, Electrooculography, Eye tracker, Human$\backslash$nlocomotion, PCPG, Prosthesis.},
