author = {Gabriel, Ronaldo C and Abrantes, Jo{\~{a}}o and Granata, Kevin and Bulas-Cruz, Jos{\'{e}} and Melo-Pinto, Pedro and Filipe, Vitor},
doi = {10.1016/j.ptsp.2007.08.002},
file = {:home/eprietop/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gabriel et al. - 2008 - Dynamic joint stiffness of the ankle during walking gender-related differences.pdf:pdf},
