keywords = {02.70.Dh,46.32.+x,62.20.M−,81.05.Qk,Compression behaviour,Folded core,Honeycomb,Imperfections,Sandwich structures,Shear behaviour,Virtual testing},
mendeley-tags = {Compression behaviour,Folded core,Honeycomb,Imperfections,Sandwich structures,Shear behaviour,Virtual testing},
month = {apr},
