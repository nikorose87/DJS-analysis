@article{Hill2013a,
abstract = {Lower-extremity amputees encounter a series of stress-related challenges. Among them is an increased risk of chronic joint disorders. For unilateral, transtibial amputees, we hypothesize that increasing the power output of the trailing, ankle-foot prosthesis during powered plantar flexion could mitigate kinetic loading applied to the leading, contralateral leg during walking. Here, we present a case series that analyzes kinetic factors of unilateral, transtibial amputee gait and forms a comparison between two types of ankle prostheses with varying power outputs. The factors examined here are impact resultant force, peak foot pressure at heel-strike, step-to-step transition work, and knee external adduction moment. The two prostheses are the amputee participant's daily-use passive ankle-foot prosthesis and the BiOM powered ankle-foot prosthesis capable of biologically accurate powered plantar flexion during late stance. In a preliminary study on two transtibial amputees walking over level terrain at a controlled speed (1.25 m/s), we observed average reductions of 8{\%} in peak impact resultant force, 18{\%} in impact resultant force loading rate, 8{\%} in peak heel-strike foot pressure, and 15{\%} in the 1(st) peak knee external adduction moment when the powered ankle-foot prosthesis was compared to the conventional passive prosthesis. Overall, our preliminary results suggest that more biomimetic prosthetic ankle-foot push-off during late stance may limit leading-leg musculoskeletal stress in walking.},
author = {Hill, David and Herr, Hugh},
doi = {10.1109/ICORR.2013.6650375},
file = {::},
isbn = {9781467360241},
issn = {19457898},
journal = {IEEE International Conference on Rehabilitation Robotics},
keywords = {amputee,ankle,biomechanics,external adduction moment,gait,loading rate,pressure,prosthesis,resultant force,transtibial},
pmid = {24187194},
title = {{Effects of a powered ankle-foot prosthesis on kinetic loading of the contralateral limb: A case series}},
year = {2013}
}
@article{Houdijk2009,
abstract = {The purpose of this study was to investigate whether the increased energy cost of amputee gait could be accounted for by an increase in the mechanical work dissipated during the step-to-step transition in walking. Eleven transtibial amputees (AMP) and 11 age-matched controls (CO) walked at both comfortable (CWS) and fixed (FWS, 1.3m/s) walking speed, while external mechanical work of each separate leg and metabolic energy consumption were measured. At FWS the metabolic energy consumption (E(met)) was significantly higher in AMP compared to CO (3.34 Jkg(-1)s(-1) vs. 2.73 Jkg(-1)s(-1)). At CWS, no difference in energy consumption was found (3.56 Jkg(-1)s(-1) vs. 3.58 Jkg(-1)s(-1)) but CWS was significantly lower in AMP compared to CO (1.35 ms(-1) vs. 1.52 ms(-1)). In conjunction with the higher E(met) at FWS, the negative work generated by the intact leading leg for the step-to-step transition in double support was significantly higher for AMP than CO at FWS. A moderate though significant correlation was found between negative mechanical power generated during the step-to-step transition and metabolic power (CWS: r=-0.56, p=0.007; FWS: r=-0.50, p=0.019). Despite the difference in negative work during the step-to-step transition, the total absolute mechanical work over a stride did not differ between groups. This could possibly be attributed to exchange of internal positive and negative work during single support, which remains unnoticed in the external work calculations. It was concluded that the increased mechanical work for the step-to-step transition from prosthetic to intact limb contributes to the increased metabolic energy cost of amputee walking.},
author = {Houdijk, Han and Pollmann, Eveline and Groenewold, Marlies and Wiggerts, Han and Polomski, Wojtek},
doi = {10.1016/j.gaitpost.2009.02.009},
file = {::},
issn = {1879-2219},
journal = {Gait {\&} posture},
keywords = {Adult,Amputee,Amputees,Amputees: rehabilitation,Anthropometry,Artificial Limbs,Case-Control Studies,Energy Metabolism,Energy Metabolism: physiology,Energy Transfer,Energy Transfer: physiology,Energy cost,Female,Gait,Humans,Lower Extremity,Lower Extremity: physiology,Male,Mechanical work,Metabolic energy,Middle Aged,Muscle Contraction,Muscle Contraction: physiology,Oxygen Consumption,Oxygen Consumption: physiology,Physical Exertion,Probability,Prosthesis Design,Prosthesis Fitting,Reference Values,Risk Assessment,Sensitivity and Specificity,Stress, Mechanical,Tibia,Tibia: surgery,Walking,Walking: physiology,Weight-Bearing},
mendeley-tags = {Amputee,Energy cost,Gait,Mechanical work,Metabolic energy},
month = {jul},
number = {1},
pages = {35--40},
pmid = {19321343},
title = {{The energy cost for the step-to-step transition in amputee walking.}},
url = {http://www.sciencedirect.com/science/article/pii/S0966636209000642},
volume = {30},
year = {2009}
