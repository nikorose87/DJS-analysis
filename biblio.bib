issn = {18792219},
journal = {Gait and Posture},
keywords = {Below knee amputee,Gait,Joint contact force,Osteoarthritis,Prosthesis},
number = {October 2016},
pages = {246--251},
pmid = {28822943},
title = {{Sound side joint contact forces in below knee amputee gait with an ESAR prosthetic foot}},
