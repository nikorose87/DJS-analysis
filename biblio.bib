journal = {Mechanics of Materials},
keywords = {Compressive behaviour,Computed tomography,FEM,Foam material,Hugoniot relation,Shock},
mendeley-tags = {Compressive behaviour,Computed tomography,FEM,Foam material,Hugoniot relation,Shock},
month = {may},
pages = {26--36},
title = {{Determination of the constitutive relation and critical condition for the shock compression of cellular solids}},
url = {http://www.sciencedirect.com/science/article/pii/S0167663616300291},
volume = {99},
year = {2016}
}
@article{Toulorge2013,
abstract = {This paper presents a technique that allows to untangle high-order/curvilinear meshes. The technique makes use of unconstrained optimization where element Jacobians are constrained to lie in a prescribed range through moving log-barriers. The untangling procedure starts from a possibly invalid curvilinear mesh and moves mesh vertices with the objective of producing elements that all have bounded Jacobians. Bounds on Jacobians are computed using the results of Johnen et al. (2012, 2013) [1,2]. The technique is applicable to any kind of polynomial element, for surface, volume, hybrid or boundary layer meshes. A series of examples demonstrate both the robustness and the efficiency of the technique. The final example, involving a time explicit computation, shows that it is possible to control the stable time step of the computation for curvilinear meshes through an alternative element deformation measure. {\textcopyright} 2013 Elsevier Inc..},
