issn = {18727646},
journal = {Human Movement Science},
keywords = {Ankle,Arch,Barefoot,Foot,Injury,Kinetics,Running},
number = {1},
pages = {147--156},
pmid = {24556475},
publisher = {Elsevier B.V.},
title = {{Ankle work and dynamic joint stiffness in high- compared to low-arched athletes during a barefoot running task}},
