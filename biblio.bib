keywords = {Biomechanics,Energy,Gait analysis,Mechanical work,Walking speed},
number = {6},
pages = {769--778},
pmid = {18384921},
title = {{Effects of cadence on energy generation and absorption at lower extremity joints during gait}},
volume = {23},
year = {2008}
}
