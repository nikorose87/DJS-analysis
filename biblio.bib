journal = {International Journal for Numerical Methods in Biomedical Engineering},
keywords = {inverse dynamics,inverse kinematics,prosthesis},
number = {4},
pages = {1--11},
title = {{Approach for gait analysis in persons with limb loss including residuum and prosthesis socket dynamics}},
