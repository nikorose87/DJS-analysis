journal = {Gait {\&} posture},
keywords = {Amputees,Artificial Limbs,Biomechanical Phenomena,DSA,Female,Gait,Gait: physiology,Hip,Hip: physiology,Humans,Low Back Pain,Low Back Pain: prevention {\&} control,Male,Motion,Osseointegration,Pelvis,Pelvis: physiology,improvements},
