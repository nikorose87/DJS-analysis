keywords = {Adult,Amputation, Traumatic,Amputation, Traumatic: rehabilitation,Ankle Joint,Ankle Joint: physiopathology,Ankle Joint: surgery,Arthroplasty, Replacement, Ankle,Biomechanical Phenomena,Female,Gait,Gait: physiology,Humans,Joint Prosthesis,Male,Prosthesis Design,Range of Motion, Articular,Walking,Walking: physiology,Young Adult},
month = {apr},
