journal = {Clinical biomechanics (Bristol, Avon)},
keywords = {Amputees,Ankle Joint,Ankle Joint: physiopathology,Artificial Limbs,Biomechanical Phenomena,Biomechanical Phenomena: physiology,Energy Metabolism,Exercise Test,Gait,Gait: physiology,Humans,Knee Joint,Knee Joint: physiopathology,Male,Middle Aged,Prosthesis Design,Tibia,Weight-Bearing,Weight-Bearing: physiology,ankle,knee,metabolic cost},
mendeley-tags = {ankle,knee,metabolic cost},
