keywords = {Adult,Biomechanical Phenomena,Bone,Computer Simulation,Flexible multibody dynamics,Humans,Imaging, Three-Dimensional,Loading,Male,Modeling,Models, Anatomic,Strain,Stress, Mechanical,Tibia,Tibia: physiology,Walking,Walking: physiology,Weight-Bearing,Weight-Bearing: physiology},
