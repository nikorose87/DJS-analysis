file = {:home/eprietop/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Zelik et al. - 2011 - Systematic variation of prosthetic foot spring affects center-of-mass mechanics and metabolic cost during walking.pdf:pdf},
isbn = {1558-0210 (Electronic)$\backslash$r1534-4320 (Linking)},
issn = {15344320},
journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
keywords = {Amputee gait,ankle push-off,prosthetic feet,prosthetic simulator},
number = {4},
pages = {411--419},
pmid = {21708509},
title = {{Systematic variation of prosthetic foot spring affects center-of-mass mechanics and metabolic cost during walking}},
volume = {19},
year = {2011}
