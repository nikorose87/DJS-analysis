keywords = {Adult,Aged,Amputees,Amputees: rehabilitation,Ankle Joint,Ankle Joint: physiopathology,Artificial Limbs,Biomechanical Phenomena,Biomechanical Phenomena: physiology,Energy Metabolism,Energy Metabolism: physiology,Exercise Test,Functional Laterality,Functional Laterality: physiology,Gait,Gait: physiology,Humans,Male,Middle Aged,Muscle, Skeletal,Muscle, Skeletal: physiology,Prosthesis Design,Walking,Walking: physiology,Weight-Bearing,Weight-Bearing: physiology,articulated ESR},
