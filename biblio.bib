issn = {10506411},
journal = {Journal of Electromyography and Kinesiology},
keywords = {Gait analysis,Idiopathic toe walking,Inverse dynamics,Juvenile idiopathic arthritis,Stiffness},
number = {4},
pages = {566--574},
pmid = {22572108},
publisher = {Elsevier Ltd},
title = {{Analytical decomposition can help to interpret ankle joint moment-angle relationship}},
