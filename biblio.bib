isbn = {1071-1007},
issn = {1071-1007},
journal = {Foot {\&} ankle international / American Orthopaedic Foot and Ankle Society [and] Swiss Foot and Ankle Society},
keywords = {amputation,diabetic foot ulcers,mos sf-36,prognosis,quality of life},
number = {11},
