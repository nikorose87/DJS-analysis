journal = {Structural and Multidisciplinary Optimization},
keywords = {Dynamic response optimization,Equivalent static loads,Equivalent static loads method for non linear stat,Nonlinear dynamic response optimization,Nonlinear static response optimization,Structural optimization for flexible multi-body dy,Structural optimization for multi-body dynamic sys},
