@phdthesis{Rosas2015,
abstract = {The use of biomechanical simulations to understand the effects of ankle-foot orthoses in chil- dren with cerebral palsy could improve medical prescription by reducing costs and controlling the possibility of iatrogenesis and posture compensation. The aim of this study is to perform a biomechanical analysis of a patient with crouching gait and a volunteer with normal gait to develop an appropriate orthotic device for the patient. The analysis has been designed using a computerised simulation with the software OpenSim, where a generic musculoskeletal model was adapted to model two types of orthoses: fixed ankle-foot orthosis (AFO) and floor reaction ankle-foot orthosis (FRO). A residual reduction algorithm was applied to minimize the modelling effects as well as the marker data processing errors. Furthermore, a computer muscle control algorithm was used after addition of external forces to resolve the cinema- tic and kinematic parameters and obtain the end results. This study shows that the use of orthoses reduces the support time and favours step symmetry. AFO and FRO devices were compared taking into account parameters such as cinematic behaviour, articular moments and degrees of rigidity. For example, it was observed that the left lower limb worked better with the FRO simulation whereas the right limb achieved a good result with the FRO simulation. Taken together, these data demonstrate that a computerised model could be used to assess the clinical conditions of the patient to design a personalised device and to anticipate and follow up the possible effects of the orthosis on a crouching gait.},
author = {Rosas, Ximena Rosas},
file = {:home/eprietop/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rosas - 2015 - An{\'{a}}lisis biomec{\'{a}}nico para prescripci{\'{o}}n ort{\'{e}}sica en postoperatorio de cirug{\'{i}}a multinivel para diplejia esp{\'{a}}stica. Es.pdf:pdf},
keywords = {Cerebral palsy,biomechanical modeling.,crouch gait,orthoses},
mendeley-tags = {Cerebral palsy,biomechanical modeling.,crouch gait,orthoses},
school = {Universidad Nacional de Colombia Bogot{\'{a}}},
title = {{An{\'{a}}lisis biomec{\'{a}}nico para prescripci{\'{o}}n ort{\'{e}}sica en postoperatorio de cirug{\'{i}}a multinivel para diplejia esp{\'{a}}stica. Estudio de caso}},
