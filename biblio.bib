@article{Hagberg2014,
abstract = {OBJECTIVE: To report outcome regarding general and specific physical health-related quality of life of treatment with percutaneous osseointegrated prostheses.$\backslash$n$\backslash$nDESIGN: Prospective two-year case-control study.$\backslash$n$\backslash$nSETTING: University hospital.$\backslash$n$\backslash$nPARTICIPANTS: 39 individuals (mean age 44±12.4) with unilateral transfemoral amputation due to trauma (n=23), tumor (n=11) or other cause (n=5). At baseline, 33/39 used socket-suspended prostheses.$\backslash$n$\backslash$nINTERVENTION: osseointegrated prosthesis.$\backslash$n$\backslash$nMAIN OUTCOME MEASURES: Questionnaire for Individuals with Transfemoral Amputation (Q-TFA), SF-36 Physical Function (PF) and Physical Component Scores (PCS), SF-6D and the Physiological Cost Index (PCI).$\backslash$n$\backslash$nRESULTS: At two years post implantation, six of seven Q-TFA scores improved (p{\textless}0.0001) compared with baseline (Prosthetic Use, Mobility, Problem, Global, Capability and Walking Habits). The Walking Aid sub-score did not improve (p=0.327). Increased prosthesis use was reported by 26/39, the same amount of use by 11/39, and less use by 2/39 subjects. Improvement was reported in 16/30 separate Problem items (p{\textless}0.05). Unchanged items included problems regarding phantom limb pain and pain from the back, shoulders and contralateral limb. The PF, PCS and SF-6D improved a mean of 24.1±21.4 (p{\textless}0.0001), 8.5±9.7 (p{\textless}0.0001) and 0.039±0.11 (p=0.007) points, respectively. Walking energy cost decreased (mean PCI: baseline 0.749, follow-up 0.610 (p{\textless}0.0001)).$\backslash$n$\backslash$nCONCLUSIONS: Two years after intervention, patients with a unilateral TFA treated with OPRA implant showed important improvements in prosthetic function and physical quality of life. However, walking aids used and the presence of phantom limb pain and pain in other extremities were unchanged. This information is valuable when considering whether a percutaneous osseointegrated prosthesis is a relevant treatment option.},
author = {Hagberg, Kerstin and Hansson, Elisabeth and Br{\aa}nemark, Rickard},
doi = {10.1016/j.apmr.2014.07.009},
file = {::},
issn = {1532-821X},
journal = {Archives of physical medicine and rehabilitation},
pmid = {25064778},
title = {{Outcome of percutaneous osseointegrated prostheses for patients with unilateral transfemoral amputation at two-year follow-up.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25064778},
year = {2014}
}
