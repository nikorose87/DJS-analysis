journal = {Journal of biomechanics},
keywords = {Adult,Ankle,Ankle: physiology,Biomechanical Phenomena,Female,Foot,Foot: physiology,Gait,Gait: physiology,Humans,Level,Male,Prosthesis,Prosthesis Design,Stairs,Walking,Walking: physiology},
mendeley-tags = {Ankle,Foot,Level,Prosthesis,Stairs},
