keywords = {Adult,Amputation Stumps,Amputation Stumps: physiopathology,Amputees,Biomechanics,Electromyography,Female,Gait,Humans,Leg,Leg: physiopathology,Male,Middle Aged,Muscle Contraction,Muscle, Skeletal,Muscle, Skeletal: physiopathology,Physical Exertion,Transtibial amputee,Walking},
